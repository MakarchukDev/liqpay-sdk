{
    "name": "makarchukdev/liqpay-sdk",
    "version": "1.0.6",
    "description": "SDK for LiqPay integration",
    "type": "library",
    "require": {
        "php": ">=7.0.0"
    },
    "autoload": {
        "psr-4": {
            "Makarchukdev\\LiqpaySdk\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Makarchukdev\\LiqpaySdk\\Providers\\LiqPayServiceProvider"
            ],
            "aliases": {
                "LiqPay": "Makarchukdev\\LiqpaySdk\\Facades\\LiqPayFacade"
            }
        }
    },
    "authors": [
        {
            "name": "Liqpay",
            "email": "support@liqpay.ua",
            "homepage": "https://www.liqpay.ua/",
            "role": "Developer"
        }
    ]
}
